{
  "name": "lt-surge",
  "version": "2.1.0",
  "description": "A collection of connect middleware to mimic to Harp/Surge serving behaviour",
  "repository": "https://github.com/fffunction/lt-surge",
  "main": "index.js",
  "scripts": {
    "test": "ava"
  },
  "keywords": [],
  "author": "Dan Reeves <danr@fffunction.co>",
  "license": "MIT",
  "dependencies": {
    "connect": "3.5.0",
    "escape-html": "1.0.3",
    "mime": "1.3.4",
    "parseurl": "1.3.1",
    "pause": "0.1.0",
    "send": "0.14.1"
  },
  "devDependencies": {
    "ava": "0.16.0",
    "connect": "3.4.1",
    "eslint": "3.5.0",
    "eslint-config-airbnb": "11.1.0",
    "eslint-plugin-import": "1.15.0",
    "eslint-plugin-jsx-a11y": "2.2.2",
    "eslint-plugin-react": "6.2.2",
    "get-port": "2.1.0",
    "node-fetch": "1.6.1"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "no-var": 0,
      "vars-on-top": 0,
      "prefer-arrow-callback": 0,
      "no-param-reassign": 0,
      "prefer-template": 0,
      "no-prototype-builtins": 0,
      "semi": [
        1,
        "always"
      ],
      "indent": [
        1,
        4
      ],
      "space-before-function-paren": [
        1,
        {
          "anonymous": "always",
          "named": "always"
        }
      ],
      "no-console": 0,
      "padded-blocks": 0,
      "no-empty": 0
    }
  }
}
