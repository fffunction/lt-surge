{
  "extends": "airbnb",
  "rules": {
    "no-var": 0,
    "vars-on-top": 0,
    "prefer-arrow-callback": 0,
    "no-param-reassign": 0,
    "prefer-template": 0,
    "no-prototype-builtins": 0,
    "object-shorthand": 0,
    "react/require-extension": 0,
    "import/extensions": [1, { "js": "never", "json": "always" }],
    "semi": [
      1,
      "always"
    ],
    "indent": [
      1,
      4
    ],
    "space-before-function-paren": [
      1,
      {
        "anonymous": "always",
        "named": "always"
      }
    ],
    "no-console": 0,
    "padded-blocks": 0,
    "no-empty": 0
  }
}
